<view class='company clearfix'>
  <view class='leftlogo fl'>
    <image src="{{companyData.logo}}" mode="aspectFill"></image>
  </view>
  <view class='rightcompany fl'>
    <view class='companyname'>{{companyData.name}}</view>
    <view class='companyaddr'>{{companyData.addr}}</view>
    <view class='companyphone' bindtap='phonecall' data-phone='{{companyData.phone}}'>{{companyData.phone}}</view>
  </view>
</view>