/* pages/Bproject/proprofect/proprofect.wxss */

.itemname {
    display: inline-block;
    width: 120rpx;
    height: 80rpx;
    line-height: 80rpx;
    margin-right: 10rpx;
}

.forminput {
    min-width: 530rpx;
    display: inline-block;
    height: 80rpx;
    line-height: 80rpx;
}

.pickerselect {
    display: block;
    width: 530rpx;
    height: 80rpx;
    line-height: 80rpx;
    border: 1px solid #e2e2e2;
    padding: 0 10px;
    border-radius: 10rpx;
}

.wx-switch-input {
    width: 45px !important;
    height: 25px !important;
    margin-top: 15rpx;
}

.wx-switch-input::before {
    width: 43px !important;
    height: 23px !important;
}

.wx-switch-input::after {
    width: 23px !important;
    height: 23px !important;
}

.submitbtn {
    display: block;
    width: 60%;
    margin: 30px auto 30px auto;
    font-size: 28rpx;
    background-color: #1e9fff;
    color: #fff;
}

.inputwrap {
    width: 140rpx;
}

.roominput {
    display: inline-block;
    max-width: 60rpx;
    min-width: 60rpx;
    padding: 0 5px;
}

.inputwrap text {
    display: inline-block;
    height: 80rpx;
    line-height: 80rpx;
    margin-left: 5rpx;
}
.uploadimg{
    display: block;
    width: 128rpx;
    height: 128rpx;
    margin: 10rpx auto;
}
.uploadnotice{
    padding: 10rpx 0;
    text-align: center;
}
