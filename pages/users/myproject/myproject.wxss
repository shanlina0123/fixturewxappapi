/* pages/usercenter/project/project.wxss */
.projectul{
    margin-top: 10rpx;
}
.projectli{
    background-color: #fff;
    padding:0 20rpx;
    margin: 20rpx 0;
}
.leftimgwrap{
    position: relative;
    width: 200rpx;
    height: 200rpx;
}
.projectimg{
    position: absolute;
    width: 200rpx;
    height: 200rpx;
    left: 0;
    top: 0;
    z-index: 1
}
.projectname{
    font-size: 30rpx;
    font-weight: bold;
}
.projectstate{
    position: absolute;
    right: 0;
    bottom: 0;
    padding: 5rpx;
    color: #fff;
    z-index: 5
}
.projectright{
  width: 495rpx;
}
.projectright view{
    margin-bottom: 15rpx
}
.projectright view:first-child{
    margin-top: 15rpx
}
.projectright view text{
    margin-right: 15rpx
}
